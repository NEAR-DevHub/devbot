{
  "db_name": "PostgreSQL",
  "query": "\n                                SELECT users.name, period_type, total_score, executed_prs, largest_score, prs_opened, prs_merged\n                                FROM user_period_data \n                                JOIN users ON users.id = user_period_data.user_id\n                                WHERE period_type = $1\n                                ORDER BY total_score DESC\n                                LIMIT $2 OFFSET $3\n                                ",
